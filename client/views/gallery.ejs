    <style>
        .gallery-container {
            min-height: 100vh;
            padding-top: 100px;
            padding-bottom: 50px;
        }
        
        .gallery-hero {
            text-align: center;
            margin-bottom: 50px;
        }
        
        .gallery-hero h1 {
            font-family: 'Fredoka One', cursive;
            font-size: 3.5rem;
            margin-bottom: 20px;
            background: linear-gradient(45deg, var(--main-blue), var(--anim-light));
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
        }
        
        .gallery-coming-soon {
            background: linear-gradient(145deg, var(--accent-dark), #03223f);
            border-radius: 20px;
            padding: 60px 40px;
            text-align: center;
            border: 1px solid rgba(77, 162, 255, 0.2);
            max-width: 800px;
            margin: 0 auto;
        }
        
        .gallery-icon {
            font-size: 4rem;
            color: var(--main-blue);
            margin-bottom: 30px;
        }
        
        .gallery-title {
            font-family: 'Fredoka One', cursive;
            font-size: 2.5rem;
            color: var(--main-blue);
            margin-bottom: 20px;
        }
        
        .gallery-description {
            font-size: 1.2rem;
            line-height: 1.8;
            margin-bottom: 40px;
            opacity: 0.9;
        }
        
        .preview-gallery {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            gap: 20px;
            margin-top: 40px;
        }
        
        .preview-meme {
            background: rgba(77, 162, 255, 0.1);
            border-radius: 15px;
            padding: 20px;
            text-align: center;
            transition: transform 0.3s ease;
        }
        
        .preview-meme:hover {
            transform: translateY(-5px);
        }
        
        .preview-meme img {
            width: 100%;
            max-width: 150px;
            border-radius: 10px;
            margin-bottom: 15px;
        }
        
        .preview-meme h4 {
            color: var(--main-blue);
            margin-bottom: 10px;
            font-size: 1.1rem;
        }
        
        .preview-stats {
            display: flex;
            justify-content: space-around;
            font-size: 0.9rem;
            opacity: 0.8;
        }
        
        .preview-stats span {
            display: flex;
            align-items: center;
            gap: 5px;
        }
        
        .stats-overview {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            gap: 20px;
            margin-bottom: 40px;
        }
        
        .stat-card {
            background: linear-gradient(145deg, var(--accent-dark), #03223f);
            border-radius: 15px;
            padding: 25px;
            text-align: center;
            border: 1px solid rgba(77, 162, 255, 0.2);
        }
        
        .stat-number {
            font-size: 2.5rem;
            font-weight: 700;
            color: var(--main-blue);
            margin-bottom: 10px;
        }
        
        .stat-label {
            font-size: 1rem;
            opacity: 0.8;
        }
    </style>
</head>
